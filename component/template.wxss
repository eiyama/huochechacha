  z-index: 9999;
}
.popup-tel-box .content{
  display: block;
  width: 540rpx;
  background: #fff;
  border-radius: 0 0 8rpx 8rpx;
  margin: 0 auto;
  padding: 30rpx 60rpx 140rpx;
  position: relative;
}
.popup-tel-box .title{
  font-size: 32rpx;
  color: #f19100;
  text-align: center;
  line-height: 112rpx;
}
.popup-tel-box input{
  height: 80rpx;
  font-size: 30rpx;
  color: #999;
  border: 1rpx solid #999;
  border-radius: 6rpx;
  padding: 0 20rpx;
  background: #f7f7f7;
}
.popup-tel-box .input-placeholder{
  font-size: 30rpx;
  color: #999;
}
.popup-tel-box .tel{
  width: 496rpx;
}
.popup-tel-box .test-code{
  width: 276rpx;
  margin: 20rpx 0 0;
}
.popup-tel-box .test-code-btn{
  position: absolute;
  top: 134rpx;
  right: 60rpx;
  width: 190rpx;
  height: 84rpx;
  line-height: 84rpx;
  border: none;
  color: #fff;
  text-align: center;
  background: #f19100;
  border-radius: 6rpx;
  font-size: 24rpx;
  padding: 0;
}
.popup-tel-box .over-code{
  color: #999;
  background: #eee;
}
.popup-tel-box .submit{
  width: 540rpx;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  background: #f19100;
  border: none;
  border-radius: 6rpx;
  margin: 60rpx 0 0;
}
.popup-tel-box  .hide-icon{
  position: absolute;
  top: 150rpx;
  right: 65rpx;
}
.popup-tel-box .test-banner{
  width: 660rpx;
