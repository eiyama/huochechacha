/* pages/lookupLocation/haveTrajectory/haveTrajectory.wxss */
page{
  height: 100%
}
.box{
  width: 750rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.map {
  flex: 1;
  width: 750rpx;
}

.parentNode {
  position: relative;
}

.sonNode {
  position: absolute;
}
.bottom-info{
  color: #333;
  width: 750rpx;
  background: #fff;
  padding: 20rpx 0;
  position: relative;
}
.p-index_css_share {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 20rpx;
  right: 30rpx;
  border-radius: 50rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.bottom-info .plate{
  font-size: 32rpx;
  line-height: 48rpx;
  padding-left: 40rpx;
}
.bottom-info .start,.bottom-info .end,.bottom-info .time-bra{
  font-size: 28rpx;
  line-height: 58rpx;
  padding-left: 40rpx;
}
.bottom-info .time-bra{
  overflow: hidden;
}
.bottom-info .time-bra view{
  float: left;
}
.btn-bar{
  border-top:1rpx solid #f19100;
  padding: 40rpx 0 0;
}
.btn{
  width: 660rpx;
  margin: 0 auto;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: #f19100;
  border-radius: 6rpx;
}
.link-index{
  width: 100rpx;
  position: absolute;
  top: 20rpx;
  right: 30rpx;
  border-radius: 50rpx;
}
.link-index image{
  width: 100rpx;
  height: 100rpx;
}